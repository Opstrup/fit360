# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'fit360' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for fit360
  pod 'Firebase'
  pod 'Firebase/Auth'

  target 'fit360Tests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase'
    pod 'Firebase/Auth'
  end

  target 'fit360UITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase'
    pod 'Firebase/Auth'
  end

end
